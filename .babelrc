{
    "presets": ["react", "env"],
    "env": {
        "development": {
            "plugins": [
                "transform-decorators-legacy",
                ["react-transform", 
                {"transforms": [
                    {
                       "transform": "react-transform-hmr",
                       "imports": ["react"],
                       "locals": ["module"]
                    }
                ]}
                ]
            ]
        }
    }
}
